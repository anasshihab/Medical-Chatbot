# قاعدة البيانات - Database Configuration
# قم بإنشاء قاعدة بيانات PostgreSQL وضع بيانات الاتصال هنا
DATABASE_URL=postgresql://username:password@localhost:5432/medical_chatbot

# JWT Configuration
# يمكنك توليد مفتاح سري باستخدام: openssl rand -hex 32
SECRET_KEY=your-secret-key-here-replace-with-random-string
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# OpenAI Configuration
# احصل على API Key من: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your-openai-api-key-here

# WebTeb Symptom Checker API
# احصل على بيانات API من WebTeb
WEBTEB_API_KEY=your-webteb-api-key-here
WEBTEB_API_URL=https://api.webteb.com/symptom-checker

# Application Settings
ENVIRONMENT=development
DEBUG=True
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
